      "source": [
        "class flatten(nn.Module):\n",
        "  def forward( self, x ):\n",
        "    a,b,c,d = x.shape\n",
        "    return x.view( a,-1 )\n"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
